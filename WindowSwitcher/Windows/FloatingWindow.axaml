<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:windowSwitcher="clr-namespace:WindowSwitcher"
        xmlns:models="clr-namespace:WindowSwitcherLib.Models;assembly=WindowSwitcherLib"
        Icon="/Assets/WS_logo.ico"
        ShowInTaskbar="False"
        ShowActivated="False"
        mc:Ignorable="d"
        x:Class="WindowSwitcher.FloatingWindow"
        Title="FloatingWindow"
        Topmost="True"
        CanResize="True"
        Resized="FloatingWindowResized"
        PointerReleased="WindowPointerReleased"
        Closing="FloatingWindowClosing"
        SystemDecorations="Full"
        x:DataType="windowSwitcher:MainWindow">
        <Canvas Name="WindowCanvas" Background="Black" PointerPressed="CanvasPointerPressed" PointerReleased="CanvasPointerReleased">
                <Canvas.ContextMenu>
                        <ContextMenu Name="FloatingWindowContextMenu"></ContextMenu>
                </Canvas.ContextMenu>
                <Label FontSize="10" Foreground="Fuchsia" FontWeight="Bold" Name="WindowLabel">window name</Label>
        </Canvas>
</Window>
