<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Icon="/Assets/WS_logo.ico"
        mc:Ignorable="d"
        Width="300" Height="400" 
        MinWidth="300" MaxWidth="300" 
        MinHeight="200"
        CanResize="True"
        Topmost="True"
        x:Class="WindowSwitcher.PrefixesWindow"
        Title="PrefixesWindow">
    <StackPanel Orientation="Vertical" Name="PrefixesStackPanel">
        <StackPanel Orientation="Horizontal" Spacing="2">
            <TextBox Name="PrefixTextBox" HorizontalAlignment="Left" Width="250"></TextBox>
            <Button Name="PrefixButton" HorizontalAlignment="Right" Content="Add" Click="AddPrefixClick" HotKey="Enter"></Button>
        </StackPanel>
        <ListBox Name="PrefixListBox" SelectionChanged="PrefixListBoxSelectionChanged"></ListBox>
    </StackPanel>
</Window>
