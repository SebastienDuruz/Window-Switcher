<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:models="clr-namespace:WindowSwitcherLib.Models;assembly=WindowSwitcherLib"
        mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="200" CanResize="True"
        Width="300" Height="200"
        x:Class="WindowSwitcher.MainWindow"
        Topmost="True"
        Title="WindowSwitcher"
        x:DataType="models:WindowConfig">
        <DockPanel Margin="12 0">
                <TabControl Margin="0" Padding="0" TabStripPlacement="Top">
                        <TabItem Header="Windows" FontSize="14" Margin="2 0" Padding="0">
                                <StackPanel Name="WindowStackPanel" Orientation="Vertical"  Margin="8 0" Spacing="0">
                                        
                                </StackPanel>
                        </TabItem>
                        <TabItem Header="Settings" FontSize="14" Margin="2 0" Padding="0">
                                <StackPanel Orientation="Vertical" Margin="8 0" Spacing="8">
                                        <TextBlock>This is the second page in the TabControl.</TextBlock>
                                </StackPanel>
                        </TabItem>
                </TabControl>
        </DockPanel>
</Window>
